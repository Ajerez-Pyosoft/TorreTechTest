{
    "version": 2,
    "name": "TorreTest",
    "builds": [
        {
            "use": "@now/static-build",
            "src": "package.json"
        }
    ],
    "routes": [
        {
            "src": "(.*).js",
            "dest": "/1.js"
        }, 
        {
            "src": "(.*).json",
            "dest": "/1.js"
        },
        {
            "src": "/.*", 
            "dest": "/index.html"
        }
    ]
}